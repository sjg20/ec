/dts-v1/;
#include <st/f1/stm32f103Xg.dtsi>

/ {
	model = "Google Chameleon";
	compatible = "st,stm32f103ze", "st,stm32f103";

	chosen {
		zephyr,console = &usart1;
		zephyr,shell-uart = &usart1;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};

	leds {
		compatible = "gpio-leds";
		power_good_led: led {
			gpios = <&gpiob 14 GPIO_ACTIVE_HIGH>;
			label = "PowerGood";
		};
	};

	zephyr,user {
		sysmon_sel_gpios = <&gpioc 6 GPIO_ACTIVE_HIGH>;
		sd_mux_sel_gpios = <&gpioe 0 GPIO_ACTIVE_HIGH>;
		sd_mux_en_l_gpios = <&gpioe 1 GPIO_ACTIVE_HIGH>;
		usd_pwr_sel_gpios = <&gpiob 13 GPIO_ACTIVE_HIGH>;
		usd_pwr_en_gpios = <&gpioe 4 GPIO_ACTIVE_HIGH>;
		usd_cd_det_gpios = <&gpioe 2 GPIO_ACTIVE_HIGH>;
	};

};

&usart1 {
	current-speed = <115200>;
	status = "okay";
};

&usart2 {
	current-speed = <115200>;
	status = "okay";
};

&uart4 {
	current-speed = <115200>;
	status = "okay";
};

&uart5 {
	current-speed = <115200>;
	status = "okay";
};

&spi1 {
	status = "okay";
};

&i2c1 {
	status = "okay";
};

&i2c2 {
	status = "okay";
};

&adc1 {
	status = "okay";
};

&adc3 {
	status = "okay";
};

&usb {
	status = "okay";
};

&timers1 {
	status = "okay";
};

&systick {
	status = "okay";
};
